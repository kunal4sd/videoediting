<div
    name="global_template_alert_playlist"
    class="list-group-item list-group-item-action list-group-item-success cursor-pointer"
    data-poster="{% if playlist.poster %}{{ playlist.poster }}{% endif %}"
    data-url="{% if playlist.url %}{{ playlist.url }}{% endif %}"
>
    <div class="row">
    <div class="col-sm-5 text-right" name="start_day">
        {% if playlist.start_day %}
            {{ playlist.start_day }}
        {% endif %}
    </div>
    <div class="col-sm-2 text-center">-</div>
    <div class="col-sm-5 text-left" name="end_day">
        {% if playlist.end_day %}
            {{ playlist.end_day }}
        {% endif %}
    </div>
    </div>
    <div class="row">
    <div class="col-sm-5 text-right" name="start_hour">
        {% if playlist.start_hour %}
            {{ playlist.start_hour }}
        {% endif %}
    </div>
    <div class="col-sm-2 text-center"></div>
    <div class="col-sm-5 text-left" name="end_hour">
        {% if playlist.end_hour %}
            {{ playlist.end_hour }}
        {% endif %}
    </div>
    </div>
</div>